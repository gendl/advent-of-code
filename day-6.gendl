(in-package :gdl-user)
(gdl:define-package :aoc2022-day-6)
(in-package :aoc2022-day-6)

(defparameter *source-home* (glisp:source-pathname))

(define-object detector ()

  :input-slots ((signal-length 14))
  :computed-slots
  ((datastream ;;"mjqjpqmgbljsphdztnvjfqwrcgsmlb"
    ;;"bvwbjplbgvbhsrlpgdmjqwftvncz"
    ;;"nppdvjthqldpwncqszvftbrmjlhg"
    ;;"nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg"
    ;;"zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw"

    ""
    
               )

   (marker (let ((length (length (the datastream))))
             (dotimes (i length)
               (when (< i (+ length (the signal-length)))
                 (let ((substring (subseq (the datastream) i (min (+ i (the signal-length))
                                                                  length))))
                   (when (the (all-different? substring))
                     (return (+ i (the signal-length) )))))))))

  :functions
  ((all-different?
    (string)
    (let ((chars (remove-duplicates (coerce string 'list))))
        (= (length string) (length chars))))))
      
      
