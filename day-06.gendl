(in-package :gdl-user)(gdl:define-package :aoc2022-day-6)
(in-package :aoc2022-day-6)



(defun part-1 () (theo (make-object 'detector) marker))

(defun part-2 () (theo (make-object 'detector :signal-length 14) marker))



(defparameter *puzzle-input* nil)

(define-object detector ()

  :input-slots ((puzzle-input *puzzle-input*)(signal-length 4))
  
  :computed-slots
  ((datastream (the puzzle-input))

   (marker (let ((length (length (the datastream))))
             (dotimes (i length)
               (when (< i (+ length (the signal-length)))
                 (let ((substring (subseq (the datastream) i (min (+ i (the signal-length))
                                                                  length))))
                   (when (the (all-different? substring))
                     (return (+ i (the signal-length) )))))))))

  :functions
  ((all-different?
    (string)
    (let ((chars (remove-duplicates (coerce string 'list))))
        (= (length string) (length chars))))))
      
      

(setq *puzzle-input* "")
