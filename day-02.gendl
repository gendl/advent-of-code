(in-package :gdl-user)(gdl:define-package :aoc2022-day-2)(in-package :aoc2022-day-2)

(defparameter *puzzle-input-test* nil)(defparameter *puzzle-input* nil)



(defun part-1 ()
  (theo (make-object 'rock-paper-scissors :puzzle-input *puzzle-input*)
        strategy-guide-score))

(defun part-2 ()
  (theo (make-object 'rock-paper-scissors :puzzle-input *puzzle-input* :puzzle-part 2)
        strategy-guide-score))



(define-object rock-paper-scissors ()

  :input-slots ((puzzle-part 1) (puzzle-input *puzzle-input-test*))

  :computed-slots ((strategy-guide-score (let ((total 0))
                                           (dolist (move (the moves) total)
                                             (let ((score (the (score move))))
                                               (incf total score)))))

                   (moves (mapcar #'(lambda(string)
                                      (mapcar #'make-keyword (read-from-string (string-append "(" string ")"))))
                                  (glisp:split-regexp "\\n" (the puzzle-input)))))

  :functions ((score (move)
                     (destructuring-bind (him me) move
                       (setq him (ecase him (:a :rock) (:b :paper) (:c :scissors)))
                       (ecase (the puzzle-part)
                         (1 (setq me (ecase me (:x :rock) (:y :paper) (:z :scissors))))
                         (2 (setq me (ecase me (:x :lose) (:y :draw) (:z :win)))
                          (setq me (ecase me
                                     (:lose (ecase him (:rock :scissors) (:paper :rock) (:scissors :paper)))
                                     (:draw him)
                                     (:win (ecase him (:rock :paper) (:paper :scissors) (:scissors :rock)))))))
                       (ecase me
                         (:rock (ecase him
                                  (:rock (+ 1 3))(:paper (+ 1 0))(:scissors (+ 1 6))))
                         (:paper (ecase him
                                   (:rock (+ 2 6))(:paper (+ 2 3))(:scissors (+ 2 0))))
                         (:scissors (ecase him
                                      (:rock (+ 3 0))(:paper (+ 3 6 ))(:scissors (+ 3 3)))))))))

                         

